{
  "manifest_version": 3,
  "name": "Auto updater",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*",
    "http://generativelanguage.googleapis.com/*"
  ],

  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["content.js"]
    }
  ]
}
